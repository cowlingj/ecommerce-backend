{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "enabled": {
      "required": true,
      "default": true,
      "type": "boolean"
    },
    "description": {
      "type": "string"
    }
  }
}