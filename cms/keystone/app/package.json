    "@keystonejs/logger": "^5.1.2",
    "@types/connect-mongo": "^3.1.3",
    "@types/express-session": "^1.17.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "openid-client": "^3.15.2",
