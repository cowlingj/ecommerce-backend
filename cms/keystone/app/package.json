    "@types/node": "^14.0.10",
    "@types/passport": "^1.0.3",
    "@types/pino": "^6.0.1",
    "@types/webpack": "^4.41.17",
    "@types/webpack-node-externals": "^1.7.1",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
