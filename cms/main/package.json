{
  "name": "ecommerce-backend-cms",
  "description": "A CMS powered by keystonejs",
  "private": true,
  "version": "5.0.0",
  "author": "Jonathan Cowling",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "build": "babel --copy-files --extensions \"js,.ts\" --out-dir ./intermediates/babel ./src/ && keystone build --entry ./intermediates/babel/index.js --out ./dist",
    "start": "keystone start --entry dist/index.js",
    "tsc": "tsc",
    "clean": "rm -r dist/ intermediates/ || exit 0"
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^5.0.0",
    "@keystonejs/app-admin-ui": "^5.0.0",
    "@keystonejs/app-graphql": "^5.0.0",
    "@keystonejs/auth-password": "^5.0.0",
    "@keystonejs/fields": "^5.0.0",
    "@keystonejs/keystone": "^5.0.0",
    "@keystonejs/logger": "^5.0.0",
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/keystonejs__adapter-mongoose": "^5.1.0",
    "@types/keystonejs__app-admin-ui": "^5.1.0",
    "@types/keystonejs__app-graphql": "^5.0.0",
    "@types/keystonejs__auth-password": "^5.0.0",
    "@types/keystonejs__keystone": "^5.2.0",
    "@types/keystonejs__logger": "^5.0.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
